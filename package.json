{
  "name": "dioxus-prototype",
  "version": "0.1.1-SNAPSHOT",
  "description": "CroftSoft Dioxus Prototype",
  "scripts": {
    "prerender": "cargo run --bin prerender --features=prerender",
    "prettier": "prettier static --ignore-path .prettierignore --write",
    "serve": "http-server static -o",
    "start": "npm run prerender && npm run prettier && npm run static && npm run serve",
    "static": "cp -r public/* static"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/david-wallace-croft/dioxus-prototype.git"
  },
  "keywords": [
    "Dioxus"
  ],
  "author": "David Wallace Croft <david@CroftSoft.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/david-wallace-croft/dioxus-prototype/issues"
  },
  "homepage": "https://github.com/david-wallace-croft/dioxus-prototype#readme",
  "devDependencies": {
    "http-server": "^14.1.1",
    "prettier": "3.0.2"
  }
}
